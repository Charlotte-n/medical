{
	"easycom": {
		"autoscan": true,
		"custom": {
		"^u--(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
		"^up-(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
		"^u-([^-].*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue"
		}
	},
	"lazyCodeLoading": "requiredComponents",

	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/more/more"
		},
		{
			"path": "pages/mine/mine",
			"style": {
				"navigationBarTitleText": "我的"
			}
		}

	],
	"subPackages": [
		{
			"root": "subpages",
			"pages": [
				{
					"path": "c-pages/showImage",
                  "style": {
                    "navigationBarTitleText": "结果",
                    "enablePullDownRefresh": true
                  }

				},
				{
					"path": "TransferPage/tran_cam",
                  "style": {
                    "navigationBarTitleText": "使用须知",
                    "enablePullDownRefresh": true
                  }
				},
				{
					"path": "TransferPage/TransferPage",
                  "style": {
                    "navigationBarTitleText": "预览",
                    "enablePullDownRefresh": true
                  }
				},
				{
					"path": "subPage2/c-pages/admin/admin",
					"style": {
						"navigationBarTitleText": "管理员入口",
						"enablePullDownRefresh": true
					}

				},
				{
					"path": "subPage2/c-pages/profile/profile",
					"style": {
						"navigationBarTitleText": "我的资料",
						"enablePullDownRefresh": true
					}
				},
				{
					"path": "subPage2/c-pages/test/test",
					"style": {
						"navigationBarTitleText": "评测记录",
						"enablePullDownRefresh": true
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"selectedColor":"#409EFF",
		"color":"grey",
		"list": [
			{
				"text": "首页",
				"pagePath": "pages/home/home",
				"iconPath": "static/tab/home01.png",
				"selectedIconPath": "static/tab/home02.png"
			},
			{
				"text": "我的",
				"pagePath": "pages/mine/mine",
				"iconPath": "static/tab/mine01.png",
				"selectedIconPath": "static/tab/mine02.png"
			}
		],
		"backgroundColor": "white"
	}
}
